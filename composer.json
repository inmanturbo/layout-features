{
    "name": "inmanturbo/layout-features",
    "description": "Layout feature for inmanturbo/features and the laravel-starter-kits",
    "type": "library",
    "require": {
        "inmanturbo/features": "^0.03.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Inmanturbo\\LayoutFeatures\\": "src/"
        }
    },
    "authors": [
        {
            "name": "inmanturbo"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Inmanturbo\\LayoutFeatures\\LayoutFeaturesServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "laravel/pint": "^1.24",
        "orchestra/testbench": "^10.6"
    }
}
